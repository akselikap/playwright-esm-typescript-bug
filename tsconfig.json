{
    "compilerOptions": {
        "jsx": "react",
        "module": "NodeNext",
        "moduleResolution": "NodeNext",
        "allowSyntheticDefaultImports": true,
        "esModuleInterop": true,
        "target": "es5",
        "skipLibCheck": true,
        "lib": [
            "DOM",
            "ES5",
            "ScriptHost",
            "ES2015.Promise"
        ],
        "noUnusedLocals": true
    }
}